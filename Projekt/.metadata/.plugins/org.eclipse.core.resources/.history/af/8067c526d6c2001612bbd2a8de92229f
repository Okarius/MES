package Model;

import java.io.BufferedWriter;
import java.io.FileNotFoundException;
import java.io.FileWriter;
import java.io.IOException;
import java.util.ArrayList;

import Services.CSVEntry;

public class CSVWorker {
	String path;

	public CSVWorker(String _path) {
		// TODO Auto-generated constructor stub
	}

	public void writeToCSV(ArrayList<CSVEntry> CSVEntries) {
		BufferedWriter bw = new BufferedWriter(new FileWriter("phoneNumbers.csv"));

		String csvString = "";
		for (CSVEntry e : CSVEntries) {
			csvString = e.objectToLine();
			bw.write(csvString);

		}

		try {

		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}

	}
}
