		inquiredDevices = new ArrayList<String>();
	}
	/*
	*This funktion performs inquire
	*It is called after "inquire" button is pressed
	*/
	public void inquire(){
		inquiredDevices.add("lol");
	}
	
	//this funktion resets model
	public void reset(){
		
	}
	
}
